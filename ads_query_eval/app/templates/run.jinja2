{% extends 'base.jinja2' %}

{% block header %}
 {% include 'evals_summary.jinja2' %}
{% endblock %}

{% block content %}
  <div class="p-4">
    {% include 'eval_form.jinja2' %}
  </div>
    <div class="p-4">
        <p>Evaluations:</p>
        {% for eval in evals %}
            <div style="margin: 1em 0; border: solid thin gray">{% include 'eval.jinja2' %}</div>
        {% else %}
            No evalutions submitted.
        {% endfor %}
    </div>
{% endblock %}